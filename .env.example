# Offbeat microservice environment example

# Redis connection
OFFBEAT_REDIS_URL=redis://localhost:6379/0
OFFBEAT_ANALYSIS_QUEUE=audio_analysis_queue
OFFBEAT_RESULTS_QUEUE=audio_results_queue

# Worker runtime
OFFBEAT_THREADS=3
OFFBEAT_LOG_LEVEL=INFO

# Audio defaults (future phases)
OFFBEAT_SAMPLE_RATE=22050
OFFBEAT_SILENCE_TOP_DB=40
OFFBEAT_MIN_TRACK_DURATION_SEC=120
OFFBEAT_HOP_LENGTH=512
OFFBEAT_ENABLE_SPLEETER=true
